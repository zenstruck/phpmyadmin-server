{
    "name": "zenstruck/phpmyadmin-server",
    "description": "Run phpMyAdmin in the background with a PHP webserver",
    "keywords": ["phpmyadmin", "mysql", "webserver"],
    "homepage": "https://github.com/zenstruck/phpmyadmin-server",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Zenstruck\\PMA\\Server\\Command\\": "src/" }
    },
    "bin": ["bin/phpmyadmin"],
    "require": {
        "php": ">=7.4",
        "symfony/web-server-bundle": "^4.4",
        "symfony/filesystem": "^5.4",
        "symfony/console": "^5.4"
    },
    "require-dev": {
        "laravel-zero/phar-updater": "^1.1.1",
        "phpstan/phpstan": "^1.8"
    }
}
